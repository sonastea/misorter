{
  "name": "misorter",
  "version": "2.2.4",
  "private": true,
  "scripts": {
    "dev": "concurrently -n worker,vite -c blue,green \"bun run dev:worker\" \"bun run dev:client\"",
    "dev:client": "vite",
    "dev:worker": "wrangler dev",
    "dev:pages": "wrangler pages dev",
    "build": "vite build",
    "build:worker": "wrangler deploy --dry-run",
    "build:pages": "bun build",
    "deploy:worker": "wrangler deploy",
    "deploy:worker:preview": "wrangler deploy --env preview",
    "deploy:pages": "bun run build:pages && wrangler pages deploy dist --branch production",
    "deploy:pages:preview": "bun run build:pages && wrangler pages deploy dist --branch preview",
    "deploy": "bun run deploy:worker && bun run deploy:pages",
    "deploy:preview": "bun run deploy:worker:preview && bun run deploy:pages:preview",
    "preview": "vite preview",
    "lint": "eslint .",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.9",
    "@headlessui/tailwindcss": "^0.2.2",
    "@supabase/supabase-js": "^2.95.3",
    "@tanstack/react-query": "^5.90.20",
    "@tanstack/react-router": "^1.159.5",
    "@tanstack/router-devtools": "^1.159.5",
    "@trpc/client": "^11.10.0",
    "@trpc/react-query": "^11.10.0",
    "@trpc/tanstack-react-query": "^11.10.0",
    "@upstash/ratelimit": "^2.0.8",
    "crypto-random-string": "^5.0.0",
    "dom-to-image-more": "^3.7.2",
    "drizzle-orm": "^0.45.1",
    "ioredis": "^5.9.2",
    "postgres": "^3.4.8",
    "react-toastify": "^11.0.5",
    "superjson": "^2.2.6",
    "superstruct": "^2.0.2",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@eslint/compat": "^2.0.2",
    "@eslint/js": "^9.39.2",
    "@tailwindcss/postcss": "^4.1.18",
    "@tanstack/router-plugin": "^1.159.5",
    "@trpc/server": "^11.10.0",
    "@types/bun": "^1.3.9",
    "@types/cors": "^2.8.19",
    "@types/dom-to-image": "^2.6.7",
    "@types/node": "^25.2.3",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.4",
    "concurrently": "^9.2.1",
    "cors": "^2.8.6",
    "dotenv-cli": "^11.0.0",
    "drizzle-kit": "^0.31.9",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "nanoid": "^5.1.6",
    "postcss": "^8.5.6",
    "prettier": "^3.8.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.55.0",
    "vite": "^7.3.1",
    "vite-tsconfig-paths": "^6.1.0",
    "wrangler": "^4.64.0"
  }
}
