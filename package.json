{
  "name": "misorter",
  "version": "1.9.0",
  "private": true,
  "scripts": {
    "dev": "concurrently -n worker,vite -c blue,green \"npm run dev:worker\" \"npm run dev:client\"",
    "dev:client": "vite",
    "dev:worker": "wrangler dev -c src/server/wrangler.jsonc",
    "dev:pages": "wrangler pages dev",
    "build": "vite build",
    "build:worker": "wrangler deploy -c ./src/server/wrangler.jsonc --dry-run",
    "build:pages": "npm run build",
    "deploy:worker": "wrangler deploy -c ./src/server/wrangler.jsonc",
    "deploy:worker:preview": "wrangler deploy -c ./src/server/wrangler.jsonc --env preview",
    "deploy:pages": "npm run build:pages && wrangler pages deploy --branch production",
    "deploy:pages:preview": "npm run build:pages && wrangler pages deploy --branch preview",
    "deploy": "npm run deploy:worker && npm run deploy:pages",
    "deploy:preview": "npm run deploy:worker:preview && npm run deploy:pages:preview",
    "preview": "vite preview",
    "lint": "eslint .",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.9",
    "@headlessui/tailwindcss": "^0.2.2",
    "@tanstack/react-query": "^5.90.8",
    "@tanstack/react-router": "^1.135.2",
    "@tanstack/router-devtools": "^1.135.2",
    "@trpc/client": "^11.7.1",
    "@trpc/react-query": "^11.7.1",
    "@trpc/tanstack-react-query": "^11.7.1",
    "@upstash/ratelimit": "^2.0.7",
    "@upstash/redis": "^1.35.6",
    "crypto-random-string": "^5.0.0",
    "dom-to-image-more": "^3.7.2",
    "drizzle-orm": "^0.44.7",
    "ioredis": "^5.8.2",
    "react-toastify": "^11.0.5",
    "superjson": "^2.2.5",
    "superstruct": "^2.0.2",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@tailwindcss/postcss": "^4.1.17",
    "@tanstack/router-plugin": "^1.114.4",
    "@trpc/server": "^11.7.1",
    "@types/cors": "^2.8.19",
    "@types/dom-to-image": "^2.6.7",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.4",
    "@types/react-dom": "^19.2.3",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^5.1.1",
    "concurrently": "^9.2.1",
    "cors": "^2.8.5",
    "drizzle-kit": "^0.31.6",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "nanoid": "^5.1.6",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwindcss": "^4.1.17",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.2",
    "vite-tsconfig-paths": "^6.0.0-beta.4",
    "wrangler": "^4.47.0"
  }
}
