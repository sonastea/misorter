{
  "name": "misorter",
  "version": "2.1.2",
  "private": true,
  "scripts": {
    "dev": "concurrently -n worker,vite -c blue,green \"npm run dev:worker\" \"npm run dev:client\"",
    "dev:client": "vite",
    "dev:worker": "wrangler dev",
    "dev:pages": "wrangler pages dev",
    "build": "vite build",
    "build:worker": "wrangler deploy --dry-run",
    "build:pages": "npm run build",
    "deploy:worker": "wrangler deploy",
    "deploy:worker:preview": "wrangler deploy --env preview",
    "deploy:pages": "npm run build:pages && wrangler pages deploy dist --branch production",
    "deploy:pages:preview": "npm run build:pages && wrangler pages deploy dist --branch preview",
    "deploy": "npm run deploy:worker && npm run deploy:pages",
    "deploy:preview": "npm run deploy:worker:preview && npm run deploy:pages:preview",
    "preview": "vite preview",
    "lint": "eslint .",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.9",
    "@headlessui/tailwindcss": "^0.2.2",
    "@supabase/supabase-js": "^2.89.0",
    "@tanstack/react-query": "^5.90.12",
    "@tanstack/react-router": "^1.143.4",
    "@tanstack/router-devtools": "^1.143.4",
    "@trpc/client": "^11.8.1",
    "@trpc/react-query": "^11.8.1",
    "@trpc/tanstack-react-query": "^11.8.1",
    "@upstash/ratelimit": "^2.0.7",
    "crypto-random-string": "^5.0.0",
    "dom-to-image-more": "^3.7.2",
    "drizzle-orm": "^0.45.0",
    "ioredis": "^5.8.2",
    "postgres": "^3.4.7",
    "react-toastify": "^11.0.5",
    "superjson": "^2.2.6",
    "superstruct": "^2.0.2",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@tailwindcss/postcss": "^4.1.18",
    "@tanstack/router-plugin": "^1.141.8",
    "@trpc/server": "^11.8.1",
    "@types/cors": "^2.8.19",
    "@types/dom-to-image": "^2.6.7",
    "@types/node": "^24.10.4",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^5.1.2",
    "concurrently": "^9.2.1",
    "cors": "^2.8.5",
    "dotenv-cli": "^11.0.0",
    "drizzle-kit": "^0.31.8",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "nanoid": "^5.1.6",
    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "tailwindcss": "^4.1.17",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.1",
    "vite": "^7.2.7",
    "vite-tsconfig-paths": "^6.0.3",
    "wrangler": "^4.53.0"
  }
}
